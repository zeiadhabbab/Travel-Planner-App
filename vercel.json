{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/$1"
      }
    ],
    "functions": {
      "server/index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }